  "bugs": {
    "url": "https://github.com/quirimmo/react-qbootstrap-typescript-generator/issues"
  },
  "homepage": "https://github.com/quirimmo/react-qbootstrap-typescript-generator#readme",
  "devDependencies": {
    "jest": "^23.6.0"
  },
  "dependencies": {
    "download-github-repo": "^0.1.4",
    "yargs": "^12.0.2"
  }
}
